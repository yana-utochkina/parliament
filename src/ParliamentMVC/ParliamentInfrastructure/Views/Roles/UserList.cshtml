@model IEnumerable<ParliamentInfrastructure.Models.User>

<h2>Список користувачів</h2>

<table class="table">
	@foreach(var user in Model)
	{
		<tr>
			<td>@user.Email</td>
			<td>
				<a class="btn btn-sm btn-primary" asp-action="Edit" asp-route-userId="@user.Id">
					Права доступу
				</a>
			</td>
		</tr>
	}
</table>

